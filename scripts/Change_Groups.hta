  Q = MsgBox("Opravdu si pøejete smazat skupinu " & group & "?", vbYesNo, "Dotaz")
  If Q = vbYes Then  
    Set objComputer = GetObject("WinNT://" & strComputer & "")
    objComputer.Delete "group", group
    If Err.Number = 0 Then
      iT = "Skupina " & group & " byla odstranìna."
      MsgBox iT, vbOKOnly, "Úspìch!"  
      mS(iT)  
      readGroups_onclick()               
      stCh =  "Delete_Group;" & strComputer & "\" & group & ";;;;Success"
      StoreChanges (stCh)
    Else  
      iT = "Skupina nebyla odstranìna." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number) 
      MsgBox iT, vbOKOnly, "Chyba!" 
      mS(iT)  
      stCh =  "Delete_Group;" & strComputer & "\" & group & ";;;;Fail"
      StoreChanges (stCh)
    End If
  End If    
End Function

Function CreateNewGroup()  
On Error Resume Next 
  if Len(newItemNameValue) > 2 And Len(newItemDescValue) > 5 Then
    Set colAccounts = GetObject("WinNT://" & strComputer & "")
    Set objUser = colAccounts.Create("group", newItemNameValue)
    objUser.Description = newItemDescValue
    objUser.SetInfo
    If Err.Number = 0 Then
      iT = "Skupina " & newItemNameValue & " byla vytvoøena."
      MsgBox iT, vbOKOnly, "Úspìch!"  
      mS(iT)  
      readGroups_onclick()  
      stCh =  "Create_Group;" & strComputer & "\" & newItemNameValue & ";;;;Success"
      StoreChanges (stCh)
    Else
      iT = "Skupina nebyla vytvoøena." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number)   
      MsgBox iT, vbOKOnly, "Chyba!"
      mS(iT)  
      stCh =  "Create_Group;" & strComputer & "\" & newItemNameValue & ";;;;Fail"
      StoreChanges (stCh)
    End If
  Else
    iT = "Délka jména skupiny musí být delší než 2 znaky." & vbCrLf & "Délka popisu musí být delší než 5 znakù."   
    MsgBox iT, vbOKOnly, "Chyba zadání"     
    mS(iT)  
  End If
End Function
  
Function CreateNewUser()       
On Error Resume Next 
  If PasswordStrengthCheck(newItemPassValue) >= 3 And Len(newItemPassValue) > 9 Then
    If Len(newItemNameValue) > 2 And Len(newItemDescValue) > 5 Then
      Set colAccounts = GetObject("WinNT://" & strComputer & "")
      Set objUser = colAccounts.Create("User", newItemNameValue)     
      objUser.FullName = newItemFullValue
      objUser.Description = newItemDescValue 
      objUser.SetPassword newItemPassValue
      objUser.SetInfo 
      If Err.Number = 0 Then
        iT = "Lokální úèet byl vytvoøen."
        MsgBox iT, vbOKOnly, "Úspìch!"
        mS(iT)    
        readMemgers()                    
        stCh =  "Create_User;" & strComputer & "\" & user & ";;;;Success"
        StoreChanges (stCh)
      Else   
        iT = "Lokální úèet nebyl vytvoøen." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number)
        MsgBox iT, vbOKOnly, "Chyba!"
        mS(iT)    
        stCh =  "Create_User;" & strComputer & "\" & user & ";;;;Fail"
        StoreChanges (stCh)
      End If
    Else
      iT = "Délka uživatelského jména musí být delší než 2 znaky." & vbCrLf & "Délka popisu musí být delší než 5 znakù."
      MsgBox iT , vbOKOnly, "Chyba zadání" 
      mS(iT)  
    End If
  Else 
    iT = "Heslo nesplòuje požadavky na složitost."  
      MsgBox iT, vbOKOnly, "Chyba zadání" 
      mS(iT)  
  End If
End Function

Function AddUserToGroup()    
'On Error Resume Next        
  strComputer = document.getElementById("cn").value
  group = document.getElementById("groups").value
  toAdd = ""
  w = "user"
  
  If group <> "Local Users" And Len(group) > 1 Then
    If InStr(1, newItemNameValue, "\", 1) Then 
      TMP = Split(newItemNameValue, "\")
      usrIsFrom = TMP(0) 
      usrNameIs = TMP(1)  
      tmp = UCase(newItemNameValue)
      tmp = Replace(tmp,"CZSKDA01\", "")   
      If Left(tmp, 4) = "APP." OR Left(tmp, 6) = "SKODA." OR Left(tmp, 4) = "E4T." OR Left(tmp, 4) = "SAS." Then 
        w = "group"     
      ElseIf Left(tmp, 3) = "DZC" OR (Left(tmp, 1) = "D" And IsNumeric(Right(tmp, 6)) = True) Then
        w = "user"   
      Else
        MsgBox "Chyba"
      End If               
    Else
      usrIsFrom = strComputer 
      usrNameIs = newItemNameValue  
    End If    
    Set objGroup = GetObject("WinNT://" & strComputer & "/" & group & ",group")
    Set objUser = GetObject("WinNT://" & usrIsFrom & "/" & usrNameIs & "," & w)
    objGroup.Add(objUser.ADsPath)
    If Err.Number = 0 Then
      iT = "Uživatel " & newItemNameValue & " byl pøidán do skupiny " & group & "."  
      MsgBox iT, vbOKOnly, "Úspìch!" 
      mS(iT)    
      stCh =  "Add_User;" & strComputer & "\" & group & "\" & usrNameIs & ";;;;Success"
      StoreChanges (stCh)
      readMemgers() 
    Else   
      iT = "Uživatel nebyl pøidán do skupiny." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number)
      MsgBox iT, vbOKOnly, "Chyba!"
      mS(iT)  
      stCh =  "Add_User;" & strComputer & "\" & group & "\" & usrNameIs & ";;;;Fail"
      StoreChanges (stCh)
    End If    
  Else
    MsgBox "Vyberte správnou skupinu.", vbOKOnly, "Chyba"
  End If
End Function

Function DelUserFromGroup()    
On Error Resume Next 
  d = 0       
  strComputer = document.getElementById("cn").value
  group = document.getElementById("groups").value   
  User = document.getElementById("users").value   
  
  w = "user"
  If group <> "Local Users" Then
    If left(UCase(User), 3) = "APP" Then 
      d = 1
      w = "group"
    End If
    If left(UCase(User), 6) = "SKODA." Then
      d = 1
      w = "group"
    End If
    If left(UCase(User), 4) = "E4T." Then
      d = 1
      w = "group"
    End If
    If left(UCase(User), 4) = "SAS." Then
      d = 1
      w = "group"
    End If
    If left(UCase(User), 3) = "DZC" And Len(user) = 7 Then
      d = 1
      w = "user"
    End If
    If left(UCase(User), 1) = "D" And IsNumeric(Right(User, 6)) = True Then
      d = 1
      w = "user"
    End If
    
    If d = 1 Then
      domain = "CZSKDA01"
    Else
      domain = strComputer
    End If 
  
    Set objGroup = GetObject("WinNT://" & strComputer & "/" & group & ",group")
    Set objUser = GetObject("WinNT://" & domain & "/" & User & "," & w)
    objGroup.Remove(objUser.ADsPath)
    If Err.Number = 0 Then
      iT = "Uživatel " & newItemNameValue & " byl odebrán ze skupiny " & group & "."
      MsgBox iT, vbOKOnly, "Úspìch!"
      mS(iT) 
      readMemgers()    
    stCh =  "Remove_User;" & strComputer & "\" & group & "\" & user & ";;;;Success"
    StoreChanges (stCh)
    Else   
      iT = "Uživatel nebyl odebrán skupiny." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number)
      MsgBox iT, vbOKOnly, "Chyba!"  
      mS(iT) 
    stCh = "Remove_User;" & strComputer & "\" & group & "\" & user & ";;;;Fail"
    StoreChanges (stCh)
    End If
  Else
    MsgBox "Vyberte správnou skupinu.", vbOKOnly, "Chyba"
  End If
End Function

