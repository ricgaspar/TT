  If Q = vbYes Then  
    Set objComputer = GetObject("WinNT://" & strComputer & "")
    objComputer.Delete "user", user
    If Err.Number = 0 Then
      iT = "Uživatel " & user & " byl odstranìn." 
      MsgBox iT, vbOKOnly, "Úspìch!"  
      mS(iT)  
      stCh =  "Delete_User;" & strComputer & "\" & user & ";;;;Success"
      StoreChanges (stCh)
      readMemgers() 
    Else  
      iT = "Uživatel nebyl odstranìn." & vbCrLf & vbCrLf & "Chyba: " & vbCrLf & errChk(Err.Number) 
      MsgBox iT, vbOKOnly, "Chyba!"    
      mS(iT)  
      stCh =  "Delete_User;" & strComputer & "\" & user & ";;;;Fail"
      StoreChanges (stCh)
    End If    
  End If
End Function

